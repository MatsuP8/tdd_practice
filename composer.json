{
    "name": "matsup8/tdd_practice",
    "type": "project",
    "license": "proprietary",
    "description": "tdd practice",
    "require": {
        "php": "7.3.12"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5",
        "phpstan/phpstan": "^0.12.2"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\": "tests/"
        }
    },
    "scripts": {
        "test": [
            "phpunit"
        ],
        "stan": "phpstan analyse"
    },
    "scripts-descriptions": {
        "test": "Run unit tests",
        "stan": "Run PHPstan analyse"
    }
}
